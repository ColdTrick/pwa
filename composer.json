{
	"name": "coldtrick/pwa",
	"description": "Progressive Web App implementation",
	"type": "elgg-plugin",
	"keywords": ["elgg", "plugin", "pwa"],
	"license": "GPL-2.0-only",
	"support": {
		"source": "https://github.com/ColdTrick/pwa",
		"issues": "https://github.com/ColdTrick/pwa/issues"
	},
	"require": {
		"composer/installers": "^1.0.8",
		"npm-asset/upup": "~1.1.0"
	},
	"require-dev": {
		"coldtrick/sniffs": "dev-master",
		"coldtrick/releases": "dev-master"
	},
	"conflict": {
		"elgg/elgg": "<3.3"
	},
	"config": {
        "process-timeout": 0,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
